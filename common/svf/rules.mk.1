
# $(LIBSVF)
$(COMMON_SVF_LIB) : $(COMMON_SVF_OBJS) 
	if test ! -d $(SOCBLOX_LIBDIR); then mkdir -p $(SOCBLOX_LIBDIR); fi
	$(LINK) -o $@ $(DLLOUT) $(COMMON_SVF_OBJS) $(LIBSVF_LINK)

$(SOCBLOX_OBJDIR)/%.o : $(COMMON_SVF_DIR)/%.cpp
	if test ! -d $(SOCBLOX_OBJDIR); then mkdir -p $(SOCBLOX_OBJDIR); fi
	$(CXX) -c -o $(SOCBLOX_OBJDIR)/$*.o $(CXXFLAGS) $(COMMON_SVF_DIR)/$*.cpp

	